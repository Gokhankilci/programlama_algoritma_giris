sayi1=int(input("1.sayıyı gir:"))
sayi2=int(input("2.sayıyı gir:"))
sayi3=int(input("2.sayıyı gir:"))
if sayi1>sayi2:
    if sayi1>sayi3:
        print("en büyük sayı:",sayi1)
    else:
        print("en büyük sayı:",sayi3)
elif sayi2>sayi3:
    print("en büyük sayı:",sayi2)
else:
    print("en büyük sayı:",sayi3)
